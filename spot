@commands.cooldown(1, 900, commands.BucketType.user)
@client.command()
async def spotify(ctx):
    await ctx.channel.purge(limit=1)
    author  = ctx.message.author
    account = urllib.request.urlopen('https://pastebin.com/raw/zFmz7xgn').read().splitlines()
    account = random.choice(account)
    account=str(account,'utf-8')
    embed = discord.Embed(
        colour = discord.Colour.green()
    )
    embed = discord.Embed(colour=0xff2b2b)
    embed.set_author(name="SimpleGenerator", icon_url="https://cdn.discordapp.com/attachments/626461260044697611/642380236415041537/Unbenannt-1.png")
    embed.add_field(name="**Account**", value=f"{account}", inline=False)
    embed.add_field(name="Want this bot in your server?", value="https://direct-link.net/28958/Bot", inline=False)
    await ctx.author.send(embed=embed)
    table_instance = SingleTable([[(datetime.datetime.now()),'\033[31m' + "[/spotify]" +'\033[39m' " command used by", author, "account he got:", account]], 'Acccount')
    print(table_instance.table)
    await ctx.send(f"{author} 📤 I sent a Spotify account to you!")
    time.sleep(5)
    await ctx.channel.purge(limit=1)

@spotify.error
async def spotify_error(ctx, error):
    author = ctx.message.author
    if isinstance(error, commands.CommandOnCooldown):
        await ctx.send("🚨 please wait before generate a new account")
        time.sleep(5)
        await ctx.channel.purge(limit=1)

