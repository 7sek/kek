from discord.ext import commands, tasks
from terminaltables import DoubleTable
from terminaltables import SingleTable
from discord.ext.commands import has_permissions, MissingPermissions

table_data = [
    ['Name >    ', 'SimpleGenerator'],
    ['ID   >    ', '629656985213861899'],
]

client = commands.Bot(command_prefix = '/')
client.remove_command("help")
status = cycle(['SimpleCrack.Net', 'Account Generator', '@Simple#1548'])

@client.event
async def on_ready():
    change_status.start()

    title= '\033[32m' + 'By yCode & BLZKRG' + '\033[39m'
    table = DoubleTable(table_data, title)
    print (table.table)
    print(Style.RESET_ALL)

@tasks.loop(seconds=10)
async def change_status():
    await client.change_presence(activity=discord.Game(next(status)))

@client.command()
async def help(ctx):
    await ctx.channel.purge(limit=1)
    embed = discord.Embed(colour=0xff2b2b,
                          description="When I reach 200 subscriber I do add Fortnite accounts! /yt" "\n"
                                      "You can buy the premium generator for only 5€ by asking Simple#1548 " "\n"
                                      "You can generate more accounts and you have a 5min cooldown and some more options",
                          title="» /gen" "\n"
                          "» /bot" "\n"
                          "» /yt" "\n"
                          "» /shop" "\n"
                          )
    embed.set_author(name="SimpleGenerator", icon_url="https://cdn.discordapp.com/attachments/626461260044697611/642380236415041537/Unbenannt-1.png")
    await ctx.send(embed=embed)
    time.sleep(10)
    await ctx.channel.purge(limit=1)

